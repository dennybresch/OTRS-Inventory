                <dl class="FieldExplanation">
                    <dt><code>$Text{"Type"}</code></dt>
                    <dd>
                        $Text{"Type of the object."} <br>
                        ($Text{"e. g."}
                        <code>$Text{"PC/Monitor/Printer"}</code>).
                    </dd>

                    <dt><code>$Text{"Model"}</code></dt>
                    <dd>
                        $Text{"Model is the Version of the Object."} 
                        <br>
                        ($Text{"e. g."}
                        <code>$Text{"PC 2307/Printer 0815"}</code>).
                    </dd>
                    
                    <dt><code>$Text{"Manufacturer"}</code></dt>
                    <dd>
                        $Text{"Company that manufactures a particular item."} <br>
                        ($Text{"e. g."}
                        <code>$Text{"Dell"}</code>).
                    </dd>

                    <dt><code>$Text{"Serialnumber"}</code></dt>
                    <dd>
                        $Text{"String that is a unique identifier of an object."} <br>
                        ($Text{"e. g."} 
						<code>$Text{"12345-45678-23070"}</code>).
                    </dd>
                    
                    <dt><code>$Text{"PurchaseTime"}</code></dt>
                    <dd>
                        $Text{"Date of purchase"}. <br>
                        ($Text{"e. g."} 
						<code>$Text{"23.07.1988"}</code>).
                    </dd>
                    
                    <dt><code>$Text{"Comment"}</code></dt>
                    <dd>
                        $Text{"Use the comment field for some additional information."} <br>
                        ($Text{"e. g."} 
                        <code>$Text{"This notification informs all agents about the coming update of OTRS to version 3.3.3."}</code>).
                    </dd>
                </dl> 
