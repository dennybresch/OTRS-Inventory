				<ul class="ActionList">
	                	<li>
					    	<button type="submit" class="CallForAction  Fullsize  Center"  
					    	onclick="if (getElementById('ID').value=='$Text{"UB-ID"}') getElementById('ID').value='';
					    	if (getElementById('Type').value=='$Text{"Type"}') getElementById('Type').value='';
					    	if (getElementById('Model').value=='$Text{"Model"}') getElementById('Model').value='';
					    	if (getElementById('Manufacturer').value=='$Text{"Manufacturer"}') getElementById('Manufacturer').value='';
					    	if (getElementById('Serialnumber').value=='$Text{"Serialnumber"}') getElementById('Serialnumber').value='';
					    	if (getElementById('SAP').value=='$Text{"SAP"}') getElementById('SAP').value='';
					    	if (getElementById('Room').value=='$Text{"Room"}') getElementById('Room').value='';
					    	" ><span>$Text{"Search"}</span></button>  					
	                    </li>
	                    <li>
				 			<button type="submit" value="$Text{"reset"}"  class="CallForAction  Fullsize  Center" 
