					    		getElementById('Type').value='Type';
					    		getElementById('Model').value='Model';
					    		getElementById('Manufacturer').value='Manufacturer';
					    		getElementById('Serialnumber').value='Serialnumber';
					    		getElementById('SAP').value='SAP';
					    		getElementById('Room').value='Room';								    		
					    		"><span>$Text{"reset"}</span></button> 
				   		</li>
	            </ul>
