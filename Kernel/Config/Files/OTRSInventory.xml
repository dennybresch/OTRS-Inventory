                    <NavBar>Inventory</NavBar>
                    <Prio>200</Prio>
                    <Type></Type>
                    <Block></Block>
                    <AccessKey></AccessKey>
                </NavBar>
                <NavBar>
                    <Group>inventory</Group>
                    <Description Translatable="1">All</Description>
                    <Name Translatable="1">All</Name>
                    <Link>Action=AgentInventory;Subaction=Overview;Limit=9999;</Link>
                    <LinkOption></LinkOption>
                    <NavBar>Inventory</NavBar>
                    <Prio>300</Prio>
                    <Type></Type>
                    <Block></Block>
                    <AccessKey></AccessKey>
                </NavBar>
                <Loader>
                    <CSS>Core.Inventory.css</CSS>
                </Loader>
            </FrontendModuleReg>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="Inventory###Additional" Required="1" Valid="1" ConfigLevel="100">
        <Description Translatable="1">Defines some addtionals.</Description>
        <Group>Inventory</Group>
        <SubGroup>Frontend::Agent::Inventory</SubGroup>
         <Setting>
            <Array>
                <Item>employee</Item>
                <Item>room</Item>
                <Item>phone</Item>
            </Array>
        </Setting>
    </ConfigItem>
</otrs_config>
