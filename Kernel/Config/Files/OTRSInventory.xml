                    <Type>Menu</Type>
                    <Prio>8900</Prio>
                    <Block>ItemArea</Block>
                    <AccessKey>i</AccessKey>
                </NavBar>
                <NavBar>
                    <GroupRo>inventory</GroupRo>
                    <Description Translatable="1">Overview</Description>
                    <Name Translatable="1">Overview</Name>
